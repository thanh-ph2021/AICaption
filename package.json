{
  "name": "Status & Bio AI Generator",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "build-staging": "copy .env.staging .env && react-native run-android",
    "build-prod": "copy .env.prod .env && react-native run-android",
    "android:releaseBundle": "copy .env.prod .env && cd android && gradlew bundleRelease && cd ..",
    "android:releaseApk": "copy .env.staging .env && cd android && gradlew assembleRelease && cd ..",
    "export-android": "scripts\\export-android.cmd",
    "compress-archive": "scripts\\compress-archive.cmd",
    "export-ios": "mkdir -p ios/output && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/output/main.jsbundle --assets-dest ios/output --sourcemap-output ios/sourcemap.js && cd ios && find output -type f | zip main.jsbundle.zip -@ && zip sourcemap.zip sourcemap.js && cd .. && rm -rf ios/output && rm -rf ios/sourcemap.js"
  },
  "dependencies": {
    "@google/genai": "^0.7.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-navigation/bottom-tabs": "^7.3.9",
    "@react-navigation/native": "^7.1.5",
    "@react-navigation/native-stack": "^7.3.9",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "grapheme-splitter": "^1.0.4",
    "i18next": "^25.0.0",
    "moment": "^2.30.1",
    "nanoid": "^5.1.5",
    "react": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.78.2",
    "react-native-blob-util": "^0.21.2",
    "react-native-config": "^1.5.5",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-google-mobile-ads": "^15.3.1",
    "react-native-image-picker": "^8.2.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-notifier": "^2.0.0",
    "react-native-ota-hot-update": "^2.2.4",
    "react-native-reanimated": "^3.17.3",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.10.0",
    "react-native-share": "^12.0.9",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.11.2",
    "react-redux": "^9.2.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
